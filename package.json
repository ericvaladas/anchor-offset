{
  "name": "anchor-offset",
  "version": "1.0.5",
  "description": "Offset scroll position for page anchors",
  "repository": {
    "type": "git",
    "url": "https://github.com/ericvaladas/anchor-offset.git"
  },
  "main": "dist/lib.js",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "npm run clean && webpack",
    "lint": "eslint src",
    "test": "nyc --reporter=html --reporter=text mocha --compilers js:babel-register test",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "prepare": "npm run lint && npm run test && npm run build",
    "publish": "publish-release --token=$PUBLISH_RELEASE_TOKEN --notes=$npm_package_version --tag=$npm_package_version"
  },
  "keywords": [
    "anchor",
    "hash",
    "offset",
    "scroll"
  ],
  "author": "ericvaladas",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ericvaladas/anchor-offset/issues"
  },
  "homepage": "https://github.com/ericvaladas/anchor-offset",
  "publishRelease": {
    "repo": "anchor-offset",
    "owner": "ericvaladas",
    "name": "anchor-offset",
    "assets": [
      "dist/anchor-offset.js"
    ],
    "reuseRelease": true
  },
  "files": [
    "README.md",
    "dist/lib.js"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "coveralls": "^2.13.1",
    "eslint": "^4.7.2",
    "jsdom": "^11.2.0",
    "mocha": "^3.5.3",
    "nyc": "^11.2.1",
    "webpack": "^3.6.0"
  }
}
